<h1>Edit Book Detail</h1>

<%= form_for @reservation, :url =>{:action => "update", :id =>@reservation} do |f| %>

<p>Start: <%= f.text_field 'reservation_start' %></p>
<p>End: <%= f.text_field  'reservation_end' %></p>
<p>Description<br/>

<%= f.text_area 'description' %></p>
<%= f.submit "Save changes" %>
<% end %>